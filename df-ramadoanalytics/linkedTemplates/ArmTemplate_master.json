{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-ramadoanalytics"
		},
		"LS_ADLS_RAMADOANALYTICSDATA_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_RAMADOANALYTICSDATA'"
		},
		"LS_ATBL_RAMADOANALYTICSDATA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ATBL_RAMADOANALYTICSDATA'"
		},
		"LS_ADLS_RAMADOANALYTICSDATA_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ramadoanalyticsdata.dfs.core.windows.net/"
		},
		"LS_AKV_RAMADOANALYTICS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-ramadoanalytics.vault.azure.net/"
		},
		"LS_HTTP_SAP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat(linkedService().SAP_GW_HOST, linkedService().SAP_ODATA_SRV, '/')}"
		},
		"LS_HTTP_SAP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().SAP_GW_USER}"
		},
		"LS_ODATA_SAP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat(linkedService().SAP_GW_HOST, linkedService().SAP_ODATA_SRV, '/')}"
		},
		"LS_ODATA_SAP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().SAP_GW_USER}"
		},
		"pe-adlgen2-ramadoanalytics_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/9e79b470-ec7a-4a74-a8a2-09647bbd3470/resourceGroups/RAMADO-ADF/providers/Microsoft.Storage/storageAccounts/ramadoanalyticsdata"
		},
		"pe-adlgen2-ramadoanalytics_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"pe-adlgen2-ramadoanalytics_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"ramadoanalyticsdata.dfs.core.windows.net"
			]
		},
		"pe-storage-ramadoanalytics_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/9e79b470-ec7a-4a74-a8a2-09647bbd3470/resourceGroups/RAMADO-ADF/providers/Microsoft.Storage/storageAccounts/ramadoanalyticsdata"
		},
		"pe-storage-ramadoanalytics_properties_groupId": {
			"type": "string",
			"defaultValue": "blob"
		},
		"pe-storage-ramadoanalytics_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"ramadoanalyticsdata.blob.core.windows.net"
			]
		},
		"pe-tablestorage-ramadoanalytics_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/9e79b470-ec7a-4a74-a8a2-09647bbd3470/resourceGroups/RAMADO-ADF/providers/Microsoft.Storage/storageAccounts/ramadoanalyticsdata"
		},
		"pe-tablestorage-ramadoanalytics_properties_groupId": {
			"type": "string",
			"defaultValue": "table"
		},
		"pe-tablestorage-ramadoanalytics_properties_fqdns": {
			"type": "array",
			"defaultValue": [
				"ramadoanalyticsdata.table.core.windows.net"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-ramadoanalytics_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_RAMADOANALYTICSDATA_accountKey": {
						"value": "[parameters('LS_ADLS_RAMADOANALYTICSDATA_accountKey')]"
					},
					"LS_ATBL_RAMADOANALYTICSDATA_connectionString": {
						"value": "[parameters('LS_ATBL_RAMADOANALYTICSDATA_connectionString')]"
					},
					"LS_AKV_RAMADOANALYTICS_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_RAMADOANALYTICS_properties_typeProperties_baseUrl')]"
					},
					"pe-adlgen2-ramadoanalytics_properties_privateLinkResourceId": {
						"value": "[parameters('pe-adlgen2-ramadoanalytics_properties_privateLinkResourceId')]"
					},
					"pe-adlgen2-ramadoanalytics_properties_groupId": {
						"value": "[parameters('pe-adlgen2-ramadoanalytics_properties_groupId')]"
					},
					"pe-adlgen2-ramadoanalytics_properties_fqdns": {
						"value": "[parameters('pe-adlgen2-ramadoanalytics_properties_fqdns')]"
					},
					"pe-storage-ramadoanalytics_properties_privateLinkResourceId": {
						"value": "[parameters('pe-storage-ramadoanalytics_properties_privateLinkResourceId')]"
					},
					"pe-storage-ramadoanalytics_properties_groupId": {
						"value": "[parameters('pe-storage-ramadoanalytics_properties_groupId')]"
					},
					"pe-storage-ramadoanalytics_properties_fqdns": {
						"value": "[parameters('pe-storage-ramadoanalytics_properties_fqdns')]"
					},
					"pe-tablestorage-ramadoanalytics_properties_privateLinkResourceId": {
						"value": "[parameters('pe-tablestorage-ramadoanalytics_properties_privateLinkResourceId')]"
					},
					"pe-tablestorage-ramadoanalytics_properties_groupId": {
						"value": "[parameters('pe-tablestorage-ramadoanalytics_properties_groupId')]"
					},
					"pe-tablestorage-ramadoanalytics_properties_fqdns": {
						"value": "[parameters('pe-tablestorage-ramadoanalytics_properties_fqdns')]"
					},
					"LS_ADLS_RAMADOANALYTICSDATA_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_RAMADOANALYTICSDATA_properties_typeProperties_url')]"
					},
					"LS_HTTP_SAP_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTP_SAP_properties_typeProperties_url')]"
					},
					"LS_HTTP_SAP_properties_typeProperties_userName": {
						"value": "[parameters('LS_HTTP_SAP_properties_typeProperties_userName')]"
					},
					"LS_ODATA_SAP_properties_typeProperties_url": {
						"value": "[parameters('LS_ODATA_SAP_properties_typeProperties_url')]"
					},
					"LS_ODATA_SAP_properties_typeProperties_userName": {
						"value": "[parameters('LS_ODATA_SAP_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-ramadoanalytics_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-ramadoanalytics_ArmTemplate_0')]"
			]
		}
	]
}